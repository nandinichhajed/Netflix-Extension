{
    "manifest_version": 3,
    "name": "Netflix Extension",
    "description": "This is an Extension for Netflix modification",
    "version": "1.0",
    "author": "Nandini Chhajed",
    "icons": {
        "128": "images/netflix.png"
      },
    "content_scripts": [
        {
            "js": ["index.js"],
            "css": ["style.css"],
            "matches": [
                "https://www.netflix.com/*/login",
                "https://www.netflix.com/*/Login",
                "https://www.netflix.com/login",
                "https://www.netflix.com/Login"
            ]
        }
    ]
}
